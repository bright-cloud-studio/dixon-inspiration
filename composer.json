{
    "name":"bright-cloud-studio/dixon-inspiration",
    "description":"Customizes Contao by adding Inspiration support for Dixon.",
    "homepage": "http://brightcloudstudio.com/",
    "keywords":["dixon", "inspiration", "bright", "cloud", "studio"],
    "type": "contao-bundle",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name": "Bright Cloud Studio",
            "homepage": "https://brightcloudstudio.com",
            "email": "mark@brightcloudstudio.com",
            "role": "Developer"
        }
    ],
    "support":{
        "issues": "https://github.com/bright-cloud-studio/dixon-inspiration/issues",
        "source": "https://github.com/bright-cloud-studio/dixon-inspiration"
    },
    "require":{
        "php": "^7.4 || ^8.0",
        "contao/core-bundle": "4.13 - 5.3"
    },
     "extra":{
        "contao-manager-plugin": "Bcs\\DixonBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\DixonBundle\\": "src"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    }
}
