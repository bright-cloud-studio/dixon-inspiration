{
    "name":"bright-cloud-studio/dixon-inspiration",
    "description":"Customizes Contao by adding Inspiration support for Dixon.",
    "homepage": "http://brightcloudstudio.com/",
    "keywords":["dixon", "inspiration", "bright", "cloud", "studio"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Bright Cloud Studio",
            "homepage":"https://brightcloudstudio.com"
        }
    ],
    "support":{
        "issues": "https://github.com/bright-cloud-studio/dixon-inspiration/issues",
        "source": "https://github.com/bright-cloud-studio/dixon-inspiration"
    },
    "require":{
        "php": "^8.0",
        "contao/core-bundle": "^4.13",
        "menatwork/contao-multicolumnwizard-bundle": "^3.6.4"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\": [
                "system/modules/dixon_inspiration/library/Bcs"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/dixon_inspiration": "system/modules/dixon_inspiration"
            }
        }
    },
    "minimum-stability": "dev"
}
